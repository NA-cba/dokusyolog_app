<% provide(:title, '全てのユーザー ') %>
<h1>全てのユーザー</h1>

<p>ユーザーの検索</p>
<%= form_tag(users_path, :method => 'get' ) do %>
  <%= text_field_tag :search %>
  <%= submit_tag '検索する', :name => nil,class: "btn btn-lg btn-primary"  %>
<% end %>

<ul class="users">

  <%= render @users %>
</ul>



<%= will_paginate @users, :previous_label => ' < 前へ', :next_label => '次へ >' %>
